<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.database.library.bibliothque">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/lib_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Library">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"></activity>
        <activity android:name=".User_Participant"></activity>
        <activity android:name=".Change_Password"></activity>
        <activity android:name=".Return"></activity>
        <activity android:name=".Show_User_Info"></activity>
        <activity android:name=".Borrow"></activity>
        <activity android:name=".Manager"></activity>
        <activity android:name=".Show_Members"></activity>
        <activity android:name=".Add_Member"></activity>
        <activity android:name=".Add_Book"></activity>
        <activity android:name=".Delete_Member"></activity>
        <activity android:name=".Add_New_Info"></activity>
        <activity android:name=".Library_Books"></activity>
        <activity android:name=".Book_Info"></activity>
        <activity android:name=".Info"></activity>
        <activity android:name=".Borrow_Page"></activity>
        <activity android:name=".Return_Page"></activity>
        <activity android:name=".Project_Description"></activity>
        <activity android:name=".IT_Squad"></activity>
    </application>
</manifest>